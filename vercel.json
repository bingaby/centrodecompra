{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin-xyz-123",
      "dest": "public/admin-xyz-123.html"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}